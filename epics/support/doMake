# devIocStats can't handle "make -j", so we have to have do this
setenv SUPPORT `pwd`

setenv EPICS_HOST_ARCH linux-x86_64
cd ${SUPPORT}/seq-2-2-1
make -j >&../make_${EPICS_HOST_ARCH}.out
cd ${SUPPORT}/devIocStats-3-1-13
make >>&../make_${EPICS_HOST_ARCH}.out
cd ${SUPPORT}
make -j >>&make_${EPICS_HOST_ARCH}.out
